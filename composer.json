{
    "name": "php-etl/spreadsheet-plugin",
    "description": "Plugin for XLS Extractor and Loader",
    "type": "gyroscops-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "nikic/php-parser": "^4.10",
        "symfony/config": "^5.2",
        "php-etl/configurator-contracts": "^0.6@dev",
        "php-etl/satellite-toolbox": "^0.4@dev",
        "php-etl/packaging-contracts": "^0.2@dev"
    },
    "require-dev": {
        "symfony/yaml": "^5.2",
        "adlawson/vfs": "dev-develop",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "php-etl/phpunit-extension": "^0.5@dev",
        "php-etl/spreadsheet-flow": "^0.2@dev",
        "php-etl/bucket-contracts": "^0.2@dev",
        "php-etl/pipeline-contracts": "^0.4@dev",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.10",
        "infection/infection": "^0.26.18",
        "rector/rector": "^0.15"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Plugin\\Spreadsheet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Plugin\\Spreadsheet\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.5.x-dev"
        },
        "gyroscops": {
            "plugins": ["Kiboko\\Plugin\\Spreadsheet\\Service"]
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": true,
            "infection/extension-installer": true
        }
    }
}
