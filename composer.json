{
    "name": "php-etl/spreadsheet-plugin",
    "description": "Plugin for XLS Extractor and Loader",
    "type": "satellite-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "nikic/php-parser": "^4.10",
        "symfony/config": "^5.2",
        "php-etl/configurator-contracts": "^0.3.0",
        "php-etl/packaging-contracts": "^0.1.0",
        "php-etl/satellite-toolbox": "^0.1.0",
        "php-etl/bucket-contracts": "^0.1.0",
        "php-etl/pipeline": "^0.2.0",
        "php-etl/spreadsheet-flow": "^0.1.0",
        "php-etl/pipeline-contracts": "^0.1.0"
    },
    "require-dev": {
        "symfony/yaml": "^5.2",
        "adlawson/vfs": "dev-develop",
        "phpunit/phpunit": "^9.0",
        "php-etl/phpunit-extension": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Plugin\\Spreadsheet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Plugin\\Spreadsheet\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        },
        "satellite": {
            "class": "Kiboko\\Plugin\\Spreadsheet\\Service"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
